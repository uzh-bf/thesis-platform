{
  "name": "@uzh-bf/thesis-platform",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "npm run prisma:generate",
    "build": "npm-run-all build:next",
    "build:next": "next build",
    "predev": "npm-run-all prisma:generate",
    "dev": "npm-run-all --parallel dev:*",
    "dev:next": "doppler run --config dev -- next dev",
    "format": "prettier --write .",
    "lint": "next lint",
    "prisma:deploy": "doppler run --config dev -- prisma migrate deploy",
    "prisma:deploy:prod": "doppler run --config prd -- prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "doppler run --config dev -- prisma migrate dev",
    "prisma:push": "doppler run --config dev -- prisma db push",
    "prisma:reset": "doppler run --config dev -- prisma migrate reset",
    "prisma:studio": "doppler run --config dev -- prisma studio",
    "prisma:studio:prod": "doppler run --config prd -- prisma studio",
    "prisma:seed": "doppler run --config dev -- ts-node --project tsconfig.tsnode.json --esm prisma/seed.ts",
    "prisma:setup": "run-s prisma:reset prisma:push prisma:seed",
    "start": "doppler run --config dev -- next start"
  },
  "dependencies": {
    "@azure/identity": "3.1.4",
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-regular-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@microsoft/microsoft-graph-client": "3.0.5",
    "@next-auth/prisma-adapter": "1.0.6",
    "@prisma/client": "4.13.0",
    "@tanstack/react-query": "4.29.5",
    "@trpc/client": "10.23.0",
    "@trpc/next": "10.23.0",
    "@trpc/react-query": "10.23.0",
    "@trpc/server": "10.23.0",
    "@tsconfig/next": "^1.0.5",
    "@uzh-bf/design-system": "1.6.1",
    "cross-fetch": "3.1.5",
    "date-fns": "2.30.0",
    "formik": "2.2.9",
    "jsonwebtoken": "9.0.0",
    "next": "13.3.4",
    "next-auth": "4.22.1",
    "node-fetch": "3.3.1",
    "nodemailer": "6.9.1",
    "ramda": "0.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "1.12.0",
    "yup": "1.1.1"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-types": "^2.26.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@tsconfig/node18": "2.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.8.4",
    "@types/ramda": "^0.28.20",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "10.4.14",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.4",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.23",
    "prettier-plugin-organize-imports": "3.2.2",
    "prisma": "4.13.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-radix": "2.8.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.0.4"
  },
  "volta": {
    "node": "18.15.0"
  }
}

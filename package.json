{
  "name": "@uzh-bf/thesis-platform",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "npm run prisma:generate",
    "build": "npm-run-all build:next",
    "build:next": "next build",
    "predev": "npm-run-all prisma:generate",
    "dev": "npm-run-all --parallel dev:*",
    "dev:next": "doppler run --config dev -- next dev",
    "format": "prettier --write .",
    "lint": "next lint",
    "prisma:deploy": "doppler run --config dev -- prisma migrate deploy",
    "prisma:generate": "doppler run --config dev -- prisma generate",
    "prisma:migrate": "doppler run --config dev -- prisma migrate dev",
    "prisma:push": "doppler run --config dev -- prisma db push",
    "prisma:reset": "doppler run --config dev -- prisma migrate reset",
    "prisma:studio": "doppler run --config dev -- prisma studio",
    "prisma:seed": "doppler run --config dev -- ts-node --esm prisma/seed.ts",
    "prisma:setup": "run-s prisma:reset prisma:push prisma:seed",
    "start": "doppler run --config dev -- next start"
  },
  "dependencies": {
    "@azure/identity": "3.1.2",
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-regular-svg-icons": "6.2.1",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@microsoft/microsoft-graph-client": "3.0.4",
    "@next-auth/prisma-adapter": "1.0.5",
    "@next/font": "13.0.6",
    "@prisma/client": "4.7.1",
    "@tanstack/react-query": "4.19.1",
    "@trpc/client": "10.4.3",
    "@trpc/next": "10.4.3",
    "@trpc/react-query": "10.4.3",
    "@trpc/server": "10.4.3",
    "@types/ramda": "^0.28.20",
    "@uzh-bf/design-system": "0.1.3",
    "cross-fetch": "3.1.5",
    "date-fns": "2.29.3",
    "formik": "2.2.9",
    "jsonwebtoken": "8.5.1",
    "next": "13.0.6",
    "next-auth": "4.18.4",
    "node-fetch": "3.3.0",
    "nodemailer": "6.8.0",
    "ramda": "0.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "1.8.0",
    "yup": "1.0.0-beta.8"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-types": "^2.25.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "0.4.2",
    "@tailwindcss/typography": "0.5.8",
    "@tsconfig/node18": "1.0.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.8.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "10.4.13",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.19",
    "prettier-plugin-organize-imports": "3.2.1",
    "prisma": "4.7.1",
    "tailwindcss": "3.2.4",
    "tailwindcss-radix": "2.6.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.4"
  },
  "volta": {
    "node": "18.12.1"
  },
  "overrides": {
    "next-auth": {
      "next": "13.0.6"
    }
  }
}
